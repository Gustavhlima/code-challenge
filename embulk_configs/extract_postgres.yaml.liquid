in:
  type: postgresql
  host: {{ env.HOST }}
  user: {{ env.USER }}
  password: {{ env.PASSWORD }}
  database: {{ env.DATABASE }}
  table: {{ env.TABLE }}
  column_options:
    picture: 
      value_type: string
    photo:
      value_type: string
out:
  type: file
  path_prefix: {{ env.OUT_PATH_PREFIX }}
  file_ext: csv
  formatter:
    type: csv
exec:
  min_output_tasks: 1