in:
  type: file
  path_prefix: {{ env.IN_PATH_PREFIX }}
  file_ext: csv
  parser:
    charset: UTF-8
    newline: CRLF
    type: csv
    delimiter: ','
    quote: '"'
    escape: '"'
    null_string: 'NULL'
    skip_header_lines: 1
    columns: 
    - {name: customer_id, type: string} 
    - {name: customer_type_, type: string}
  
out:
  type: postgresql
  host: {{ env.HOST }}
  user: {{ env.USER }}
  password: {{ env.PASSWORD }}
  database: {{ env.DATABASE }}
  table: {{ env.TABLE }}
  port: {{ env.PORT}}
  mode: replace
  column_options:
    picture: 
      value_type: string
    photo:
      value_type: string
  