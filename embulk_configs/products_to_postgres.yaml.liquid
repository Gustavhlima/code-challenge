in:
  type: file
  path_prefix: {{ env.IN_PATH_PREFIX }}
  file_ext: csv
  parser:
    charset: UTF-8
    newline: CRLF
    type: csv
    delimiter: ','
    quote: '"'
    escape: '"'
    null_string: 'NULL'
    skip_header_lines: 1
    columns: 
    - {name: product_id, type: string} 
    - {name: product_name, type: string}
    - {name: supplier_id, type: string}
    - {name: category_id, type: string}
    - {name: quantity_per_un, type: string} 
    - {name: unit_price, type: string}
    - {name: units_in_stock, type: string}
    - {name: units_on_order, type: string}
    - {name: reorder_level, type: string} 
    - {name: discontinued, type: string}
  
out:
  type: postgresql
  host: {{ env.HOST }}
  user: {{ env.USER }}
  password: {{ env.PASSWORD }}
  database: {{ env.DATABASE }}
  table: {{ env.TABLE }}
  port: {{ env.PORT}}
  mode: replace
  column_options:
    picture: 
      value_type: string
    photo:
      value_type: string
  