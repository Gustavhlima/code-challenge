in:
  type: postgresql
  host: {{ env.HOST }}
  user: {{ env.USER }}
  password: {{ env.PASSWORD }}
  database: {{ env.DATABASE }}
  port: {{ env.PORT }}
  query: | 
    SELECT o.order_id, o.customer_id, o.employee_id, o.order_date, od.product_id, od.unit_price, od.quantity, od.discount
    FROM orders o
    JOIN order_details od ON o.order_id = od.order_id;

out:
  type: file
  path_prefix: {{ env.OUT_PATH_PREFIX }}/final_query
  file_ext: csv
  sequence_format: '.'
  formatter:
    type: csv
exec:
  min_output_tasks: 1